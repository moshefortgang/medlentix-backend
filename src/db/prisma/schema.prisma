generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["views"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model RealEstateTransactions {
  id                    Int       @id @default(autoincrement())
  createdAt             DateTime? @default(now())
  updatedAt             DateTime? @updatedAt
  gush                  Int?
  helka                 Int?
  tatHelka              Int?
  saleDate              DateTime?
  declaredValueInShekel Int?
  saleValueInShekel     Int?
  propertyType          String?
  soldPart              Int?
  locality              Int?
  constructionYear      Int?
  area                  String?
  rooms                 Int?
  blockId               Int?

  @@unique([gush, helka, tatHelka, saleDate, declaredValueInShekel, area, rooms])
}

model Project {
  id          Int       @id @default(autoincrement())
  createdAt   DateTime? @default(now())
  updatedAt   DateTime? @updatedAt
  projectName String
  shchuna     String?
  Block       Block[]
  michrazId   Int?
  city        Int?
  michraz     Michraz?  @relation(fields: [michrazId], references: [id])
}

model Block {
  id        Int       @id @default(autoincrement())
  createdAt DateTime? @default(now())
  updatedAt DateTime? @updatedAt
  projectId Int
  project   Project   @relation(fields: [projectId], references: [id])
  gush      Int?
  helka     Int?
}

model Michraz {
  id             Int       @id @default(autoincrement())
  createdAt      DateTime? @default(now())
  updatedAt      DateTime? @updatedAt
  MichrazID      Int
  MitchamName    String
  SchumZchiya    Int
  ShemZoche      String
  Kibolet        Int
  HotzaotPituach Int
  VaadaDate      DateTime
  Project        Project[]

  @@unique([MichrazID, MitchamName, SchumZchiya, ShemZoche])
}

model Cities {
  id                    Int     @id @default(autoincrement())
  settlementCode        Int
  settlementName        String
  settlementNameEnglish String
  districtCode          Int
  districtName          String
  ministryDistrictCode  Int
  ministryDistrict      String
  regionalCouncilCode   Int
  regionalCouncilName   String?
}

model RealEstateTransactionsNadlanGov {
  id                    Int      @id @default(autoincrement())
  dealDateTime          DateTime
  fullAddress           String?
  displayAddress        String?
  gush                  Int?
  helka                 Int?
  tatHelka              Int?
	dealNatureDescription String?
  assetRoomNum          String
  floorNo               String?
  dealNature            String
  dealAmount            String
  newProjectText        String?
  projectName           String?
  buildingYear          String?
  yearBuilt             String?
  buildingFloors        String?
  keyValue              String
  type                  Int
  polygonId             String
  trendIsNegative       Boolean
  trendFormat           String?
}
